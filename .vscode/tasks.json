{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build active .cpp",
      "type": "shell",
      "command": "g++",
      "args": [
        "-std=c++17",
        "-O2",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}"
      ],
      "group": "build"
    }
  ]
}
